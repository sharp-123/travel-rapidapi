    <section id="pack" class="packages">
        <div class="container-fluid" style="width: 80%;">
            <div class="gallary-header text-center">
                <h2>
                    special packages
                </h2>
                <p>
                    Duis aute irure dolor in velit esse cillum dolore eu fugiat nulla.
                </p>
            </div>
            <!--/.gallery-header-->
            <div class="packages-content">
                <div class="row">
                    <div class="col-md-3">
                        <div class="search-panel">
                            <form id="searchform">
                                <h1>Search</h1>
                                <div class="form-group form-div">
                                    <label for="hotel">Destination</label>
                                    <input class="form-control search-loc form-input" id="hotel" list="browsers"
                                        autocomplete="off" name="place_name" value="<%=params.place_name%>" />
                                    <input type="hidden" name="dest_ids" class="hotel-input"
                                        value="<%=params.dest_ids%>" />
                                    <input type="hidden" name="search_type" class="hotel-input"
                                        value="<%=params.search_type%>" />
                                </div>
                                <div class="form-group form-div">
                                    <label for="in_date">check in</label>
                                    <input type="date" name="arrival_date" id="in_date" class="form-control form-input"
                                    value="<%=params.arrival_date%>"/>
                                </div>
                                <div class="form-group form-div">
                                    <label for="out_date">check out</label>
                                    <input type="date" name="departure_date" id="out_date" class="form-control form-input" value="<%=params.departure_date%>"/>
                                </div>
                                <div class="form-group form-div">
                                    <label for="adts">Adults</label>
                                    <input class="form-control form-input" name="guest_qty" id="adts" value="<%=params.guest_qty%>">
                                </div>
                                <div class="form-group form-div">
                                    <label for="chd">Children</label>
                                    <input class="form-control form-input" name="children_qty" id="chd" value="<%=params.children_qty%>">
                                </div>
                                <div class="form-group form-div">
                                    <label for="rooms">Rooms</label>
                                    <input class="form-control form-input" name="room_qty" id="rooms" value="<%=params.room_qty%>">
                                </div>
                                <input type="hidden" name="search_id" id="searchID" class="<%=params.search_id%>"  value="<%=scrapped.search_id%>">
                                <input type="hidden" name="offset" id="offset" value=""/>
                                <input type="hidden" class="pn" name="page_number" value='<%=params.page_number%>'>
                                <datalist id="browsers">
                                </datalist>

                                <option value="San Francisco" class="999302 template-option" style="display: none;">
                                    <div class="EtzER">
                                        <div class="DvEpD">
                                            <div class="second-text">Emirate of Dubai, United Arab Emirates</div>
                                            <input type="hidden" class="search_type" />
                                        </div>
                                    </div>
                                </option>
                                <button class="btn btn-lg btn-primary form-btn" id="update" type="submit">
                                    Update
                                </button>
                            </form>
                        </div>
                        <hr>
                        <div>
                            <h4>Filter by:</h4>
                        </div>
                        <hr>
                            <div class="accordion">
                                <% scrapped.base_filters.forEach(item=>{%>
                                    <% if(item.id=="price_category") return; %>
                                        <div class="accordion__item" style="margin: 10px;">
                                            <div class="title accordion__title">
                                                <div class="accordion__arrow"><span class="accordion__arrow-item ">+</span></div> 
						                        <span class="accordion__title-text"><%=item.title%></span>
                                                <!-- <h4><%=item.title%></h4> -->
                                            </div>
                                            <div class="row accordion__content" style="font-size: 12px; font-weight: 400; margin-left: 0px !important; margin-right: 0px !important; ">
                                                <% item.categories.forEach(ct=>{%>
                                                    <div class="form-check col-md-10">
                                                        <input class="form-check-input filter_check" type="checkbox" name="categories_filter[]"  value="<%=ct.id%>" form="searchform"  <% if(ct.selected==1){%>checked<%}%>>
                                                        <label class="form-check-label" for="<%=ct.id%>" style="font-weight:400">
                                                                    <%=ct.name%>
                                                        </label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <%=ct.count%>
                                                    </div>
                                            <%  })%>
                                            </div>
                                        </div>
                                <%  })%>
                            </div>
                        <hr>
                    </div>
                    <div class="col-md-9">
                        <% scrapped.result.forEach(item=>{%>
                            <div class="col-md-12 col-sm-12">
                                <div class="single-package-item" style="min-height:200px; display: flex; padding: 1%;">
                                    <div class="col-md-3">
                                        <img src="<%=item.main_photo_url.replace('square60','max300')%>" style="width: 200px; height: 200px;"/>
                                    </div>
                                    <div class="col-md-9">
                                        <div class="col-md-8">
                                            <div class="col-md-12 "><a id="<%=item.hotel_id%>" class="property-name"><%=item.hotel_name%></a>
                                                <div class="rating" style="color: rgb(228, 218, 33); font-size: 18px;">
                                                        <%for(var i=0;i<item.class;i++){%>
                                                            <i class="fa fa-star"></i>
                                                            <%}%>
                                                    </div>
                                            </div>
                                            <div class="col-md-12 address"><a><%=item.address%>&nbsp;<%=item.distance%><mark>km</mark> away from city center</a></div>
                                        </div>
                                        <div class="col-md-4" style="padding: 0;">
                                            <div class="col-md-9">
                                                <div class="score-word"><%=item.review_score_word%></div>
                                                <div><%=item.review_nr%>&nbsp;reviews..</div>
                                                </div>
                                            <div class="col-md-3 review-score"><%=item.review_score%></div>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="room-msg"><%=item.urgency_room_msg%></div>
                                            <div>deal event</div>
                                            <% if(item.hotel_include_breakfast=='1'){%>
                                                <div class="facility-info">Breakfast&Dinner included</div>
                                                <%}%>
                                            <% if(item.is_free_cancellable=='1'){%>
                                                <div class="facility-info">FREE cancellation • No prepayment needed</div>
                                                <div class="facility-info-exp">You can cancel later, so lock in this great price today!</div>
                                                        <%}%>
                                            <% if(item.has_swimming_pool=='1'){%>
                                                                <div class="facility-info">Awesome Swimming Pool</div>
                                                                <%}%>
                                            <% if(item.has_fee_parking=='1'){%>
                                                                        <div class="facility-info">Free Parking</div>
                                                                        <%}%>
                                                                            <% if(item.urgency_message!=undefined){%>
                                            <div class="urgent-msg"><%=item.urgency_message.replace("Booking.com","our site")%></div>
                                                                                <%}%>
                                        </div>
                                        <div class="col-md-4">
                                            <div> 
                                                <div class="bk-price">price from Booking.com</div>
                                                <div class="crossed-price"><span class="cur-code"><%=item.price_breakdown.currency%> &nbsp;&nbsp;</span><%=item.price_breakdown.gross_price%></div>
                                                <div class="os-price">price from our site</div>
                                                <div class="price"><span class="cur-code"><%=item.price_breakdown.currency%> &nbsp;&nbsp;</span><%=(0.5*Number(item.price_breakdown.gross_price))%></div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/.single-package-item-txt-->
                                </div>
                                <!--/.single-package-item-->
                            </div>
                            <%})%>
                    </div>
                    <div class="col-md-12">
                        <button class="btn btn-primary form-btn btn-lg pull-left " type="submit"
                            >Prev</button>
                        <button class="btn btn-primary form-btn btn-lg pull-right " type="submit"
                            >Next</button>
                    </div>
                    <form action="/api/hotel/getHotelDetails" method="get" id="detail" style="display: none;">
                        <input type="hidden" name="hotel_id">
                        <input type="hidden" name="search_id" value="<%=scrapped.search_id%>">
                        <input type="hidden" name="departure_date" value="<%=params.departure_date%>">
                        <input type="hidden" name="arrival_date" value="<%=params.arrival_date%>">
                        <input type="hidden" name="rec_guest_qty" value="<%=params.guest_qty%>">
                        <input type="hidden" name="rec_room_qty" value="<%=params.room_qty%>">
                        <button type="submit"  id="gethotelDetails"></button>
                    </form>

                </div>
                <!--/.row-->
            </div>
            <!--/.packages-content-->
        </div>
        <!--/.container-->

    </section>
