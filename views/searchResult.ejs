    <script>
        alert("Hi my friend. This is david...I have some problem with account in Freelancer.com So I am arguing with freelancer staff.It may be take for a while. So meanwhile, please contact HERE. telegram:  @sharp_rk_123-----whatsapp:  +1 562 245 2496--------discord: Sharp#9612 I am waiting for you")
        $(document).ready(function () {
            if ($("input.pn").val() == "1") {
                $(".pull-left").attr("disabled",'true');
            }
            $("div.item:first-child").addClass("active");
            $(".property-name").click(function(){
                $("#detail input[name='hotel_id']").val($(this).attr('id'));
                $("#gethotelDetails ").click();
            })
            $(".pull-left").click(()=>{
                $("input.pn").val(Number($("input.pn").val())-1)
                $("#offset").val(20*Number($("input.pn").val()-1));
                $("#update").click();
            })
            $(".pull-right").click(()=>{
                $("#offset").val(20*Number($("input.pn").val()));
                $("input.pn").val(Number($("input.pn").val())+1)
                $("#update").click();
            })
            $('.search-loc').keydown(function () {

                let category = $(this).attr('id');
                let query = $(this).val();
                if (query.length % 3 == 0 && query.length != 0) {
                    console.log("scraping start");
                    $.ajax({
                        type: "GET",
                        url: "/api/" + category + "/searchlocation/",
                        data: { query },
                        crossDomain: true,
                        success: function (result) {
                            console.log(result);
                            $('#browsers').empty();
                            result.forEach(ele => {
                                let newOp = $("option.template-option").clone();
                                newOp.removeClass("template-option");
                                newOp.val(ele['name']);
                                newOp.attr('class', ele['dest_id']);
                                newOp.find('div.second-text').text(ele['label']);
                                newOp.find('input.search_type').val(ele['dest_type']);
                                // $("#browsers").append(newel);
                                newOp.appendTo("#browsers");
                            });
                        },
                        error: function (err) {
                            console.log(err);
                        }
                    });
                }

            });
            $(".search-loc").change(function () {
                let value = $("#browsers").find('option[value="' + $(this).val() + '"]').attr("class");
                let s_type = $("#browsers").find('option[value="' + $(this).val() + '"] input[type=hidden]').val();
                $(this).siblings('input[name="dest_ids"]').val(value);
                $(this).siblings('input[name="search_type"]').val(s_type);
                console.log($(this).siblings('input[name="dest_ids"]').val());
                console.log($(this).siblings('input[name="search_type"]').val());
            })
        })

        $(function() {
	    //BEGIN
            $(".accordion__title").on("click", function(e) {

                e.preventDefault();
                var $this = $(this);

                if (!$this.hasClass("accordion-active")) {
                    $(".accordion__content").slideUp(400);
                    $(".accordion__title").removeClass("accordion-active");
                    $('.accordion__arrow').removeClass('accordion__rotate');
                }

                $this.toggleClass("accordion-active");
                $this.next().slideToggle();
                $('.accordion__arrow',this).toggleClass('accordion__rotate');
            });
        //END
        });
    </script>

